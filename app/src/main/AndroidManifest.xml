<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seth.nelson.sportmatching">
    <uses-permission android:name="com.android.vending.BILLING" />
    <application
        android:name="seth.nelson.sportmatching.CoinFootball.MyCoinApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FootballElement">
        <activity
            android:name="seth.nelson.sportmatching.GetFootball" />
        <activity
            android:name="seth.nelson.sportmatching.GameWelcome"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="seth.nelson.sportmatching.GameEnd"/>
        <activity
            android:name="seth.nelson.sportmatching.Home3"/>
        <activity
            android:name="seth.nelson.sportmatching.Home2"/>
        <activity
            android:name="seth.nelson.sportmatching.Home1"/>
        <activity
            android:name="seth.nelson.sportmatching.GameLevel3"/>
        <activity
            android:name="seth.nelson.sportmatching.GameLevel2"/>
        <activity
            android:name="seth.nelson.sportmatching.GameLevel1"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>